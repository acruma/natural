							{{ page.footer_accesos_directos }}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	
	<div class="row ps_footerlogoSection">
		<div class="col-12">
			<div class="ps_logo">
				{{ page.footer_logo }}
			</div>
		</div>
	</div>
	
	<div class="row ps_footerMainMenuSection">
		<div class="col-12">
			<div class="ps_content_margins">
				<div class="row">
					<div class="col-12 col-md">
						{{ page.footer_first }}
