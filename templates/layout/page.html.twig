			<div class="col-12 ps_col_highlighted">
				{{ page.highlighted }}
			</div>
		</div>
	{% endif %}

	{% if page.breadcrumb %}
		<div class="row ps_breadcrumbContainer bgColorAreaTematica d-none d-sm-block">
			<div class="col-12">
				<div class="ps_content_margins">
					{{ page.breadcrumb }}
				</div>
			</div>
		</div>
	{% endif %}

	{% if page.alertas %}
		<div class="row ps_alertasContainer">
			<div class="col-12 ps_noPadding_LR">
				{{ page.alertas }}
			</div>
		</div>
	{% endif %}
	
	{% if page.contenido_extra %}
        <div class="row ps_contenido_extra_row">
            <div class="col-12 ps_contenido_extra">
                {{ page.contenido_extra }}
            </div>
        </div>
    {% endif %}

	<div class="row">
		<div class="col-12 ps_mainContent ps_content_margins">
			<div class="row">
				<div class="col-12">
					<a id="main-content" tabindex="-1"></a>
					{# link is in html.html.twig #}
					{% if page.sidebar_first %}
						<div class="row ps-alcobendas-content-container">
							<aside class="col-12 col-lg-3 layout-sidebar-first" role="complementary">
								{{ page.sidebar_first }}
							</aside>
							<div class="col-12 col-lg-9 ps-alcobendas-content">
								{{ page.content }}
							</div>
						</div>
					{% else %}
						<div class="row ps-alcobendas-content-container">
							<div class="col-12 ps-alcobendas-content">
								{{ page.content }}
							</div>
						</div>
					{% endif %}
				</div>
			</div>
		</div>
	</div>
	
	{% if page.contenido_generico %}
		<div class="row ps_ContentGenerico">
			<div class="col-12">
				<div class="row justify-content-md-center">
					<div class="col-12">
						{{ page.contenido_generico }}
					</div>
				</div>
			</div>
		</div>
	{% endif %}

	{% if page.feedback %}
		<div class="row ps_feedback_content">
			<div class="col-12">
				<div class="ps_content_margins">
					<div class="row justify-content-md-center">
						<div class="col-12">
							<div class="ps_content_margins">
								{{ page.feedback }}
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	{% endif %}

