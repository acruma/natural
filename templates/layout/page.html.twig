			<div class="ps_content_margins">
				<div class="row">
					<div class="col-xs-12 col-md">
						<div class="ps_footerListContainer ps_infoContact">
							{{ page.footer_third }}
						</div>
					</div>

					<div class="col-xs-12 col-md-6">
						<div class="ps_footerListContainer">
							<div class="row">
								<div class="col-12">
									{{ page.footer_fourth }}
								</div>
							</div>
						</div>
